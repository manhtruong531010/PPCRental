    <link href="~/Content/pe-icon-7-stroke.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <div class="sidebar" data-color="blue" data-image="" style="background-image: url(/Areas/Admins/Controllers/Image/sidebar-11726532.jpg)">
            <!--   you can change the color of the sidebar using: data-color="blue | azure | green | orange | red | purple" -->
            <div class="sidebar-wrapper" style="text-align: center center">
                <div class="logo">
                    <a href="~/Home/Index" style="font-size:20px">
                        PPC Rental
                    </a>
                </div>
                <ul class="nav">
                    <li class="active">
                        <a href="~/Agency/Agency/Index/@Session["userid"]">
                            <img src="~/Images/icon-dashboard.png" style="width:25px" />
                            <span style="margin-left:4%; font-size:16px">Dashboard</span>
                        </a>
                    </li>
                    <li>
                        <a href="~/Account/ChangePassword">
                            <img src="~/Images/icon-user.png" style="width:25px" />
                            <span style="margin-left:4%; font-size:16px">User Profile</span>
                        </a>
                    </li>
                    <li>
                        <a href="~/Agency/Agency/Index/@Session["userid"]">
                            <img src="~/Images/icon-list.png" style="width:25px" />
                            <span style="margin-left:4%; font-size:16px">List Agency Project</span>
                        </a>
                    </li>
                    <li>
                        <a href="notifications.html">
                            <img src="~/Images/icon-notification.png" style="width:25px" />
                            <span style="margin-left:4%; font-size:16px">Notifications</span>
                        </a>
                    </li>
                </ul>
            </div>

        </div>
        <div class="main-panel">
            <nav class="navbar navbar-default navbar-fixed" style="height:8%">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">Dashboard</a>
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-left">
                            <li>
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <i class="fa fa-dashboard"></i>
                                    <p class="hidden-lg hidden-md">Dashboard</p>
                                </a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <i class="fa fa-globe"></i>
                                    <b class="caret hidden-sm hidden-xs"></b>
                                    @*<span class="notification hidden-sm hidden-xs">5</span>*@
                                    <p class="hidden-lg hidden-md">
                                        5 Notifications
                                        <b class="caret"></b>
                                    </p>
                                </a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Notification 1</a></li>
                                    <li><a href="#">Notification 2</a></li>
                                    <li><a href="#">Notification 3</a></li>
                                    <li><a href="#">Notification 4</a></li>
                                    <li><a href="#">Another notification</a></li>
                                </ul>
                            </li>
                            <li>
                                <a href="">
                                    <i class="fa fa-search"></i>
                                    <p class="hidden-lg hidden-md">Search</p>
                                </a>
                            </li>
                        </ul>
                        
                        <ul class="nav navbar-nav navbar-right">    
                            <li>
                                <a href="~/Home/Index">
                                    <p>HOME PAGE</p>
                                </a>
                            </li>    
                            <li class="dropdown">     
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">         
                                    <p>     
                                        <img src="~/Images/icon-user.png" style="width:25px; margin-top:-5px"/>
                                        @Session["username"]
                                        <b class="caret"></b>
                                    </p>
                                </a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">View Profile</a></li>
                                    <li>
                                        <a href="~/Account/Logout">
                                            <p>Log out</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>                  
                        </ul>
                    </div>
                </div>
            </nav>
            <div class="content" style="margin-top:-10px">
                @RenderBody()

            </div>
            <footer class="footer">
                <div class="container-fluid">
                    <nav class="pull-left">
                        <ul>
                            <li>
                                <a href="#">
                                    Home
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Company
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Portfolio
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    Blog
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <p class="copyright pull-right">
                        &copy;
                        <script>document.write(new Date().getFullYear())</script> Lazy Team - K21T3 - Team 03
                    </p>
                </div>
            </footer>

        </div>
    </div>

</body>
